{
  "name": "cypress-cucumber-getting-started",
  "version": "1.0.0",
  "description": "Cypress project readilly preconfigured with cypress-cucumber-preprocessor and multiple-cucumber-html-reporter",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "pretest": "node ./cypress/reporting/delete-results.js",
    "report": "node ./cypress/reporting/report.js",
    "test": "cypress run --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webnexusmobile/cypress-cucumber-getting-started.git"
  },
  "keywords": [
    "cypress",
    "cucumber"
  ],
  "author": "webnexusmobile",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webnexusmobile/cypress-cucumber-getting-started/issues"
  },
  "homepage": "https://github.com/webnexusmobile/cypress-cucumber-getting-started#readme",
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/test-results/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "dependencies": {
    "cypress": "6.1.0",
    "cypress-cucumber-preprocessor": "4.0.0",
    "cypress-image-snapshot": "4.0.0",
    "cypress-wait-until": "1.7.1",
    "multiple-cucumber-html-reporter": "1.18.0"
  },
  "devDependencies": {
    "eslint": "7.12.1",
    "eslint-plugin-cypress": "2.11.2"
  }
}
